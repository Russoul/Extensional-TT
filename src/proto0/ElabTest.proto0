syntax e{≥6} * e{≥7} : e{6}

let _+_ : ℕ → ℕ → ℕ
        ≔ x ↦ y ↦ ℕ-elim (x. ?) y (x. h. S h) x

-- mult 0 y = 0
-- mult (1 + x) y = y + mult x y
let _*_ : ℕ → ℕ → ℕ
         ≔ x ↦ y ↦ ℕ-elim (x. ?) Z (x. h. y + h) x

let plus-left-Z : (x : ℕ) → Z + x ≡ x ∈ ℕ
                ≔ x ↦ J (α. h. α Z x ≡ x ∈ ℕ)
                        (J (α. h. α x ≡ x ∈ ℕ)
                           (J (α. h. α ≡ x ∈ ℕ)
                              (J (α. h. α ≡ x ∈ ℕ) Refl (sym (ℕ-β-Z (x. ?) ? (x.h. ?))))
                              (sym (Π-β (y. ?) ?))
                           )
                           (sym {? → ?} (Π-β (x. ?) ?))
                        )
                        (sym !_+_)

let plus-left-S : (t : ℕ) → (x : ℕ) → (S t + x) ≡ S (t + x) ∈ ℕ
                ≔ t ↦ x ↦ J (α. h. α (S t) x ≡ ?(t , x) ∈ ℕ)
                            (J (α. h. α x ≡ ?(t , x) ∈ ℕ)
                               (J (α. h. α ≡ ?(t , x) ∈ ℕ)
                                  (J (α. h. α ≡ ?(t , x) ∈ ℕ)
                                     (S-cong
                                       (J (α.h. ?(t , x) ≡ α t x ∈ ℕ)
                                          (J (α.h. ?(t , x) ≡ α x ∈ ℕ)
                                             (J (α.h. ?(t , x) ≡ α ∈ ℕ)
                                                Refl
                                                (sym (Π-β (z. ?) ?))
                                             )
                                             (sym {? → ?} (Π-β (z. ?) ?))
                                          )
                                          (sym !_+_)
                                       )
                                     )
                                     (sym (ℕ-β-S (x. ?) ? (x. h. ?) ?))
                                  )
                                  (sym (Π-β (z. ?) ?))
                               )
                               (sym {? → ?} (Π-β (z. ?) ?))
                            )
                            (sym !_+_)

let mult-left-Z : (x : ℕ) → Z * x ≡ Z ∈ ?
                ≔ x ↦ J (α.h. α Z x ≡ Z ∈ ?)
                        (J (α.h. α x ≡ Z ∈ ?)
                           (J (α.h. α ≡ Z ∈ ?)
                              (J (α.h. α ≡ Z ∈ ?)
                                 Refl
                                 (sym (ℕ-β-Z (x. ?) ? (x.h. ?)))
                              )
                              (sym (Π-β (x. ?) ?))
                           )
                           (sym {? → ?} (Π-β (x. ?) ?))
                        )
                        (sym !_*_)


let mult-left-S : (t : ℕ) → (x : ℕ) → (S t * x) ≡ (x + t * x) ∈ ?
                ≔ t ↦ x ↦
                          J (α.h. α (S t) x ≡ (x + t * x) ∈ ?)
                            (J (α.h. α x ≡ (x + t * x) ∈ ?)
                               (J (α.h. α ≡ (x + t * x) ∈ ?)
                                  (J (α.h. α ≡ (x + t * x) ∈ ?)
                                     (J (α. h. (x + α) ≡ ?(t , x) ∈ ?)
                                        (J (α. h. (x + α x) ≡ ?(t , x) ∈ ?)
                                           (J (α. h. (x + α) ≡ ?(t , x) ∈ ?)
                                              Refl
                                              ((happly' ? ? ? ? (happly' ? ? ? ? !_*_ t) x))
                                           )
                                           (the (? ≡ ? ∈ ? → ?) (Π-β (x. ?) t))
                                        )
                                        (Π-β (y. ?) x)
                                     )
                                     (sym (ℕ-β-S (x. ?) ? (x. h. ?) ?))
                                  )
                                  (sym (Π-β (x. ?) ?))
                               )
                               (sym {? → ?} (Π-β (x. ?) ?))
                            )
                            (sym !_*_)

