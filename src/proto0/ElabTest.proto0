let _∘_ : {A : 𝕌} → {B : 𝕌} → {C : 𝕌} → (g : B → C) → (f : A → B) → A → C
         ≔ {A} ↦ {B} ↦ {C} ↦ g ↦ f ↦ x ↦ g (f x)

let _≅_ : 𝕌 → 𝕌 → 𝕌
        ≔ A ↦ B ↦ (f : A → B) ⨯ (g : B → A) ⨯ (g ∘ f ≡ id {?} ∈ ?) ⨯ (f ∘ g ≡ id {?} ∈ ?)

