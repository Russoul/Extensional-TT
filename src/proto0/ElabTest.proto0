let id : (A : 𝕌) → A → A
       ≔ A ↦ x ↦ x

let sym : (A : 𝕌) → (a₀ : A) → (a₁ : A) → (a₀ ≡ a₁ ∈ A) → (a₁ ≡ a₀ ∈ A)
        ≔ A ↦ a₀ ↦ a₁ ↦ a ↦ J (x. e. x ≡ a₀ ∈ A) Refl a

let fun-eta : (A : 𝕌) → (B : A → 𝕌) → (f : (x : A) → B x) → ((x ↦ f x) ≡ f ∈ (x : A) → B x)
            ≔ A ↦ B ↦ f ↦ Π-η f

let fun-ext : (A : 𝕌) → (B : A → 𝕌) → (f : (x : A) → B x) → (g : (x : A) → B x) → (p : (x : A) → f x ≡ g x ∈ B x) → f ≡ g ∈ (x : A) → B x
            ≔ A ↦ B ↦ f ↦ g ↦ p ↦ J (α. h. α ≡ g ∈ ?) (J (α. h. ?(A, B, f, g, p) ≡ α ∈ ?) (Π⁼ (x. ?) (x. ?) (x. p x)) (Π-η ?)) (Π-η ?)

let happly' : (A : 𝕌) → (B : 𝕌) → (f : A → B) → (g : A → B) → (p : f ≡ g ∈ ?) → (x : A) → f x ≡ g x ∈ ?
            ≔ A ↦ B ↦ f ↦ g ↦ p ↦ x ↦ J (α.h. f x ≡ α x ∈ ?) Refl p

let happly : (A : 𝕌) → (B : A → 𝕌) → (f : (x : A) → B x) → (g : (x : A) → B x) → (p : f ≡ g ∈ ?) → (x : A) → f x ≡ g x ∈ ?
           ≔ A ↦ B ↦ f ↦ g ↦ p ↦ x ↦ J (α.h. f x ≡ α x ∈ ?) Refl p

let cong : (A : 𝕌) → (B : 𝕌) → (f : A → B) → (a₀ : A) → (a₁ : A) → (a : a₀ ≡ a₁ ∈ ?) → f a₀ ≡ f a₁ ∈ ?
         ≔ A ↦ B ↦ f ↦ a₀ ↦ a₁ ↦ a ↦ J (x. h. f a₀ ≡ f x ∈ ?) Refl a

let transp : (A : 𝕌) → (B : A → 𝕌) → (a₀ : A) → (a₁ : A) → (a : a₀ ≡ a₁ ∈ ?) → B a₀ → B a₁
           ≔ A ↦ B ↦ a₀ ↦ a₁ ↦ a ↦ t ↦ J (α. h. B α) t a

let S-cong : (x : ℕ) → (y : ℕ) → (e : x ≡ y ∈ ?) → S x ≡ S y ∈ ?
         ≔ x ↦ y ↦ e ↦ J (z. h. S x ≡ S z ∈ ?) Refl e

let plus : ℕ → ℕ → ℕ
         ≔ x ↦ y ↦ ℕ-elim (x. ?) y (x. h. S h) x

-- mult 0 y = 0
-- mult (1 + x) y = y + mult x y
let mult : ℕ → ℕ → ℕ
         ≔ x ↦ y ↦ ℕ-elim (x. ?) Z (x. h. plus y h) x

let plus-left-Z : (x : ℕ) → plus Z x ≡ x ∈ ℕ
                ≔ x ↦ J (α. h. α Z x ≡ x ∈ ℕ)
                        (J (α. h. α x ≡ x ∈ ℕ)
                           (J (α. h. α ≡ x ∈ ℕ)
                              (J (α. h. α ≡ x ∈ ℕ) Refl (sym ? ? ? (ℕ-β-Z (x. ?) ? (x.h. ?))))
                              (sym ? ? ? (Π-β (y. ?) ?))
                           )
                           (sym ? ? ? (Π-β (x. ?) ?))
                        )
                        (sym ? ? ? !plus)

-- (x ↦ y ↦ ℕ-elim (x. ℕ) y (x.h. S h) x) (S t)
-- y ↦ ℕ-elim (x. ℕ) y (x.h. S h) (S t)
-- ? : (x ↦ y ↦ ℕ-elim (x. ℕ) y (x.h. S h) x) (S t) ≡ ?
-- ((z ↦ ?₀) ?₁ ≡ ?₀[?₁/z]) ~ ((x ↦ y ↦ ℕ-elim (x. ℕ) y (x.h. S h) x) (S t) ≡ ?₃)
let plus-left-S : (t : ℕ) → (x : ℕ) → plus (S t) x ≡ S (plus t x) ∈ ℕ
                ≔ t ↦ x ↦ J (α. h. α (S t) x ≡ ?(t, x) ∈ ℕ)
                            (J (α. h. α x ≡ ?(t, x) ∈ ℕ)
                               (J (α. h. α ≡ ?(t, x) ∈ ℕ)
                                  (J (α. h. α ≡ ?(t, x) ∈ ℕ)
                                     (S-cong ? ?
                                       (J (α.h. ?(t, x) ≡ α t x ∈ ℕ)
                                          (J (α.h. ?(t, x) ≡ α x ∈ ℕ)
                                             (J (α.h. ?(t, x) ≡ α ∈ ℕ)
                                                Refl
                                                (sym ? ? ? (Π-β (z. ?) ?))
                                             )
                                             (sym ? ? ? (Π-β (z. ?) ?))
                                          )
                                          (sym ? ? ? !plus)
                                       )
                                     )
                                     (sym ? ? ? (ℕ-β-S (x. ?) ? (x. h. ?) ?))
                                  )
                                  (sym ? ? ? (Π-β (z. ?) ?))
                               )
                               (sym ? ? ? (Π-β (z. ?) ?))
                            )
                            (sym ? ? ? !plus)

let mult-left-Z : (x : ℕ) → mult Z x ≡ Z ∈ ?
                ≔ x ↦ J (α.h. α Z x ≡ Z ∈ ?)
                        (J (α.h. α x ≡ Z ∈ ?)
                           (J (α.h. α ≡ Z ∈ ?)
                              (J (α.h. α ≡ Z ∈ ?)
                                 Refl
                                 (sym ? ? ? (ℕ-β-Z (x. ?) ? (x.h. ?)))
                              )
                              (sym ? ? ? (Π-β (x. ?) ?))
                           )
                           (sym ? ? ? (Π-β (x. ?) ?))
                        )
                        (sym ? ? ? !mult)


let mult-left-S : (t : ℕ) → (x : ℕ) → mult (S t) x ≡ plus x (mult t x) ∈ ?
                ≔ t ↦ x ↦
                          J (α.h. α (S t) x ≡ plus x (mult t x) ∈ ?)
                            (J (α.h. α x ≡ plus x (mult t x) ∈ ?)
                               (J (α.h. α ≡ plus x (mult t x) ∈ ?)
                                  (J (α.h. α ≡ plus x (mult t x) ∈ ?)
                                     (J (α. h. plus x α ≡ ?(t, x) ∈ ?)
                                        (J (α. h. plus x (α x) ≡ ?(t, x) ∈ ?)
                                           (J (α. h. plus x α ≡ ?(t, x) ∈ ?) Refl ((happly' ? ? ? ? (happly' ? ? ? ? !mult t) x)))
                                           (Π-β (x. ?) t)
                                        )
                                        (Π-β (y. ?) x)
                                     )
                                     (sym ? ? ? (ℕ-β-S (x. ?) ? (x. h. ?) ?))
                                  )
                                  (sym ? ? ? (Π-β (x. ?) ?))
                               )
                               (sym ? ? ? (Π-β (x. ?) ?))
                            )
                            (sym ? ? ? !mult)
